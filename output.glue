    // Add container routes
    routes.extend(containers::routes());
    
    // Add image routes
    routes.extend(images::routes());
    
    // Add volume routes
    routes.extend(volumes::routes());
    
    // Add network routes
    routes.extend(networks::routes());
    
    // Add system routes
    routes.extend(system::routes());
    
    // Add agent routes
    routes.extend(agent::routes());
    
    routes
}
